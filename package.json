{
  "name": "localization-app",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Miguel Bento"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "jest",
    "lint": "vue-cli-service lint",
    "dev": "node ./bin/vite",
